[alias]
  a = !git add -A && git status
  c = commit -m
  ca = commit --amend
  pl = pull
  ps = push
  plre = pull --rebase
  st = status
  up = !git fetch origin && git rebase origin/master
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  pager = less -r
[diff]
  compactionHeuristic = true
[fetch]
  prune = true
[merge]
  ff = only
[push]
  default = nothing
[user]
  name = Tristan Dunn
  email = tristanzdunn@gmail.com
[url "git@github.com:clearbit"]
  insteadof = https://github.com/clearbit
[pull]
	ff = only
