[alias]
  c = commit -m
  pl = pull
  ps = push
  plre = pull --rebase
  st = status
  out = !git log `bundle exec cap deploy:revision -q`..HEAD
  ctags = !.git/hooks/ctags
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = ~/.gitignore
  pager = less -r
[init]
  templatedir = ~/.configuration/git_template
[merge]
  ff = only
[push]
  default = nothing
