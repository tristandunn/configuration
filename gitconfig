[alias]
  c = commit -m
  ca = commit --amend
  ctags = !.git/hooks/ctags
  out = !git log `bundle exec cap deploy:revision -q`..HEAD
  pl = pull
  ps = push
  plre = pull --rebase
  st = status
  up = !git fetch origin && git rebase origin/master
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  pager = less -r
[fetch]
  prune = true
[init]
  templatedir = ~/.configuration/git_template
[merge]
  ff = only
[push]
  default = nothing
[user]
  name = Tristan Dunn
  email = tristanzdunn@gmail.com
